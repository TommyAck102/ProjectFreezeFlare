--target=i686-none-linux-android
--gcc-toolchain="$(GCC_TOOLCHAIN)"
--sysroot="$(SYS_ROOT)"
-shared
-Wl,-soname,libunity.so
-Wl,--no-undefined
-Wl,--build-id
-Wl,-z,noexecstack
-Wl,--gc-sections
-fuse-ld=gold{HOST_EXE}
-Wl,--wrap,abort
"$(UNITY_LIBS)/libmain.so"
"$(UNITY_LIBS)/libswappyVk.so"
-Wl,--exclude-libs,libunityexternal.a
-L"$(GNU_STL_ROOT)/libs/x86"
-Wl,-u,JNI_OnLoad
-Wl,-u,JNI_OnUnload
-Wl,-u,UnitySendMessage
-Wl,--version-script="$(UNITY_COMMONSTATICLIBS)/../libunity.map"
-lc
-lm
-lgnustl_static
-landroid
-ldl
-llog
-lGLESv2
-lstdc++
-lz
-lEGL
-latomic

